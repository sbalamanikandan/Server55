r\n$this-&gt;_objTemplate-&gt;hideBlock(''headlines_row_1'');\r\n$this-&gt;_objTemplate-&gt;hideBlock(''headlines_row_2'');\r\n$this-&gt;_objTemplate-&gt;hideBlock(''headlines_row_3'');\r\n}\r\n$this-&gt;_objTemplate-&gt;setVariable("TXT_MORE_NEWS", $_CORELANG[''TXT_MORE_NEWS'']);\r\nreturn $this-&gt;_objTemplate-&gt;get();\r\n}\r\n\r\n<code>\r\n<strong>headlines.html:</strong>\r\n&lt;div class="news-list"&gt;\r\n&lt;div class="span8 news-left-right"&gt;\r\n&lt;div class="row"&gt;&lt;!-- BEGIN headlines_row_1 --&gt;\r\n&lt;div class="news-left"&gt;\r\n&lt;div class="news-image-block"&gt;&lt;img style="background-image: url(''{HEADLINE_IMAGE_PATH}'');" src="themes/lava/images/1x1.png" alt="" /&gt;&lt;/div&gt;</code>\r\n\r\n&lt;div class="news-text-block"&gt;&lt;img src="themes/lava/images/1x1.png" alt="" /&gt;\r\n&lt;div class="news-content-home"&gt;\r\n&lt;h3&gt;{NEWS_TITLE}&lt;/h3&gt;\r\n&lt;span class="news-date"&gt;23rd November 2015&lt;/span&gt;\r\n&lt;p class="news-detail"&gt;{NEWS_TEASER}&lt;/p&gt;\r\n\r\n&lt;/div&gt;\r\n&lt;/div&gt;\r\n&lt;a class="btn btn-primary" href="{NEWS_LINK_URL}"&gt;Read more&lt;/a&gt;\r\n\r\n&lt;/div&gt;\r\n&lt;!-- END headlines_row_1 --&gt; &lt;!-- BEGIN headlines_row_2 --&gt;\r\n&lt;div class="news-right"&gt;\r\n&lt;div class="news-text-block"&gt;&lt;img src="themes/lava/images/1x1.png" alt="" /&gt;\r\n&lt;div class="news-content-home"&gt;\r\n&lt;h3&gt;{NEWS_TITLE}&lt;/h3&gt;\r\n&lt;span class="news-date"&gt;24th November 2015&lt;/span&gt;\r\n&lt;p class="news-detail"&gt;{NEWS_TEASER}&lt;/p&gt;\r\n\r\n&lt;/div&gt;\r\n&lt;/div&gt;\r\n&lt;a class="btn btn-primary" href="{NEWS_LINK_URL}"&gt;Read more&lt;/a&gt;\r\n&lt;div class="news-image-block"&gt;&lt;img style="background-image: url(''{HEADLINE_IMAGE_PATH}'');" src="themes/lava/images/1x1.png" alt="" /&gt;&lt;/div&gt;\r\n&lt;/div&gt;\r\n&lt;!-- END headlines_row_2 --&gt;\r\n\r\n&lt;/div&gt;\r\n&lt;/div&gt;\r\n\r\n&lt;div class="span4 news-top-bottom tab"&gt;\r\n&lt;div class="row"&g